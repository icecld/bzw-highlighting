{
  "name": "bzw-highlighting",
  "displayName": "bzw-highlighting",
  "description": "Syntax highlighting for the BZFlag BZW world language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "bzw",
      "aliases": ["BZW", "bzw"],
      "extensions": [".bzw"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "bzw",
      "scopeName": "text.bzw",
      "path": "./syntaxes/bzw.tmLanguage.json"
    }]
  }
}
